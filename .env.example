# Trail Pal Configuration
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/trail_pal

# OpenRouteService API Key
# Get your free API key at: https://openrouteservice.org/dev/#/signup
ORS_API_KEY=your_api_key_here

# Overpass API endpoint (default is public, can be changed to local instance)
OVERPASS_API_URL=https://overpass-api.de/api/interpreter

# Rate limiting settings
ORS_REQUESTS_PER_MINUTE=40
ORS_REQUESTS_PER_DAY=2000

